        <AnchorPane>
               <children>
                  <Button layoutX="28.0" layoutY="23.0" mnemonicParsing="false" onAction="#onButtonAction" text="Button" />
               </children></AnchorPane>
      </content>
    </Tab>
  </tabs>
</TabPane>
