<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<VBox minHeight="250.0" minWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.jowko.jrank.desktop.feature.rules.StatisticsController">
   <children>
      <BorderPane maxHeight="30.0" styleClass="border-yellow" VBox.vgrow="ALWAYS">
         <center>
            <FlowPane hgap="10.0" vgap="10.0" BorderPane.alignment="CENTER">
               <children>
                  <Label fx:id="ruleTypeLabel" text="Rule type:" />
                  <Label fx:id="ruleType" />
                  <Label fx:id="usageTypeLabel" text="Usage type:">
                     <FlowPane.margin>
                        <Insets left="20.0" />
                     </FlowPane.margin>
                  </Label>
                  <Label fx:id="usageType" />
                  <Label fx:id="characteristicClassLabel" text="Characteristic class:">
                     <FlowPane.margin>
                        <Insets left="20.0" />
                     </FlowPane.margin>
                  </Label>
                  <Label fx:id="characteristicClass" />
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </FlowPane>
         </center>
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </BorderPane>
      <ScrollPane fitToWidth="true" styleClass="border-yellow" VBox.vgrow="ALWAYS">
         <content>
            <BorderPane style="-fx-background-color: FFFFE0;">
               <center>
                  <GridPane minHeight="280.0" BorderPane.alignment="CENTER">
                    <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" maxWidth="200.0" minWidth="150.0" />
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="300.0" />
                    </columnConstraints>
                    <rowConstraints>
                      <RowConstraints maxHeight="20.0" minHeight="20.0" vgrow="SOMETIMES" />
                      <RowConstraints maxHeight="20.0" minHeight="20.0" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="20.0" minHeight="20.0" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="20.0" minHeight="20.0" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="20.0" minHeight="20.0" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="20.0" minHeight="20.0" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="20.0" minHeight="20.0" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="20.0" minHeight="20.0" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="20.0" minHeight="20.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="20.0" minHeight="20.0" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="20.0" minHeight="20.0" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="20.0" minHeight="20.0" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="20.0" minHeight="20.0" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="20.0" minHeight="20.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <children>
                        <Label fx:id="supportLabel" text="Support:" />
                        <Label fx:id="supportExamplesLabel" text="Supporting examples:" GridPane.rowIndex="1" />
                        <Label fx:id="strengthLabel" text="Strength:" GridPane.rowIndex="2" />
                        <Label fx:id="confidenceLabel" text="Confidence:" GridPane.rowIndex="3" />
                        <Label fx:id="coverageFactorLabel" text="Coverage factor:" GridPane.rowIndex="4" />
                        <Label fx:id="coverageLabel" text="Coverage:" GridPane.rowIndex="5" />
                        <Label fx:id="coveredExamplesLabel" text="Covered examples:" GridPane.rowIndex="6" />
                        <Label fx:id="negativeCoverageLabel" text="Negative coverage:" GridPane.rowIndex="7" />
                        <Label fx:id="negativeExamplesLabel" text="Negative covered examples:" GridPane.rowIndex="8" />
                        <Label fx:id="inconsistencyMeasureLabel" text="Inconsistency measure:" GridPane.rowIndex="9" />
                        <Label fx:id="fMeasureLabel" text="f-confirmation measure:" GridPane.rowIndex="10" />
                        <Label fx:id="aMeasureLabel" text="A-confirmation measure:" GridPane.rowIndex="11" />
                        <Label fx:id="zMeasureLabel" text="Z-confirmation measure:" GridPane.rowIndex="12" />
                        <Label fx:id="lMeasureLabel" text="l-confirmation measure:" GridPane.rowIndex="13" />
                        <Label fx:id="support" GridPane.columnIndex="1" />
                        <Label fx:id="supportExamples" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                        <Label fx:id="strength" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                        <Label fx:id="confidence" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                        <Label fx:id="coverageFactor" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                        <Label fx:id="coverage" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                        <Label fx:id="coveredExamples" GridPane.columnIndex="1" GridPane.rowIndex="6" />
                        <Label fx:id="negativeCoverage" GridPane.columnIndex="1" GridPane.rowIndex="7" />
                        <Label fx:id="negativeExamples" GridPane.columnIndex="1" GridPane.rowIndex="8" />
                        <Label fx:id="inconsistencyMeasure" GridPane.columnIndex="1" GridPane.rowIndex="9" />
                        <Label fx:id="fMeasure" GridPane.columnIndex="1" GridPane.rowIndex="10" />
                        <Label fx:id="aMeasure" GridPane.columnIndex="1" GridPane.rowIndex="11" />
                        <Label fx:id="zMeasure" GridPane.columnIndex="1" GridPane.rowIndex="12" />
                        <Label fx:id="lMeasure" GridPane.columnIndex="1" GridPane.rowIndex="13" />
                     </children>
                  </GridPane>
               </center>
               <padding>
                  <Insets bottom="20.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </BorderPane>
         </content>
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </ScrollPane>
   </children>
</VBox>
